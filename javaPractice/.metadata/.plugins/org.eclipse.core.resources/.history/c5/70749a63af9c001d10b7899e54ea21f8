package miQuastion;

public class frin1 {

	public static void main(String[] args) {
		
		int ar[]= {0,-1,1,-3,3,2,-2,0,0};
//		int ans[]=new int[ar.length];
		for(int i=1;i<=3;i++) {
			for(int j=0;j<ar.length;j++) {
				int num=Math.abs(ar[j]);
				if(num==i) {
					System.out.print(ar[j]+" ");
				}
			}
			
		}
	}

}
