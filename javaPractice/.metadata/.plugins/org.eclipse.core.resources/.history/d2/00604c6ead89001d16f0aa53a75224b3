package practiceMore;

public class polymorphism {

	public static void main(String[] args) {
		parent p=new parent(23,"siddhu");
		p.student1();
		p.student1(22);
		p.student1("sager");
	}

}
class parent{
	int age;
	String name;
	parent(int age,String name){
		this.age=age;
		this.name=name;
	}
	void student1() {
		System.out.println(age+" "+name);
	}
	void student1(int age) {
		System.out.println(age+" "+name);
	}
	void student1(String name) {
		System.out.println(age+" "+name);
	}
}
class child{
	
}
